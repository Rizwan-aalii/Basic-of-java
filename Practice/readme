         Topic  switch
     
         // switch in java 
         switch (a ) {
             case 1 : System.out.println("hello");  
             break ;
             case 2 : System.out.println("namste");
             break;
             case 3 : System.out.println("salam");
             break;
             default : System.out.println("invalid number"); }
         
